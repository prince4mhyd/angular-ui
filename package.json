    "lint": "node --max_old_space_size=8192 -r ts-node/register node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json \"apps/polaris/src/**/*.ts\" \"libs/**/*.ts\" -e \"apps/polaris/src/app/ui-stubs/**/*.ts\"",
    "lint.ci": "./node_modules/.bin/nx lint && node --max_old_space_size=8192 -r ts-node/register node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json \"apps/polaris/src/**/*.ts\" \"libs/**/*.ts\" -e \"apps/polaris/src/app/ui-stubs/**/*.ts\" -o checkstyle-result.xml -t checkstyle",
    "lintfix": "./node_modules/.bin/nx lint && node --max_old_space_size=8192 -r ts-node/register node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json \"apps/polaris/src/**/*.ts\" \"libs/**/*.ts\" -e \"apps/polaris/src/app/ui-stubs/**/*.ts\" --fix",
