    "build.ci": "yarn build.app.ci",
    "build.app.ci": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build polaris --configuration=ci",
    "test": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test polaris --environment test --source-map=false",
    "test.headless": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test polaris --environment test --source-map=false --browsers ChromeHeadless",
    "test.unit": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test polaris --environment test --configuration unit --source-map=true",
