    "scripts": {
        "apiTest": "node -r ts-node/register -r tsconfig-paths/register ./api-tests/jasmine.js",
        "start": "yarn run test",
        "lint": "./node_modules/.bin/tslint -p tsconfig.json -c tslint.json \"src/**/*.ts\"",
        "lintAndTest": "yarn run lint && yarn run test",
        "test": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/protractor/bin/protractor ./protractor.conf.js",
        "stickyNotes": "yarn run lintAndTest --suite=stickyNotes",
        "nonStickyNotes": "yarn run lintAndTest --suite=nonStickyNotes",
        "dbscripts": "yarn run lintAndTest --suite=dbscripts",
        "careTeamWidget": "yarn run lintAndTest --suite=careTeamWidget",
        "all": "yarn run lintAndTest --suite=all",
        "stickyGetandPost": "yarn run apiTest --specs=./**/api-tests/clinical/stickyGetandPost.api.spec.ts",
        "stickyApiToDb": "Yarn run apiTest --specs=./**/api-tests/clinical/stickyApiToDb.api.spec.ts",
        "stickyUiToApi": "yarn run lintAndTest --suite=stickyUiToApi",
        "stickyUiToApiToDb": "Yarn run lintAndTest --suite=stickyUiToApiToDb",
        "stickyApiToUiToDb": "Yarn run lintAndTest --suite=stickyApiToUiToDb",
        "stickyToMongoDB": "Yarn run lintAndTest --suite=stickyToMongoDB"
    },
