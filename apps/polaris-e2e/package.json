        "gulp-contains": "1.2.0",
        "gulp-replace": "^0.5.4",
        "jasmine": "^3.3.0",
        "jasmine-reporters": "^2.0.0",
        "jasmine-spec-reporter": "^3.0.0",
        "jasmine2-protractor-utils": "^1.3.0",
        "jquery": "^3.4.1",
